            <enumeration value = "ACCEPT_JOIN" />
            <enumeration value = "REJECT" />
            <enumeration value = "DESTROY" />
            <enumeration value = "FULL" />
            <enumeration value = "INVALID_LEVEL" />
            <enumeration value = "CHANGE_OWNER" />
            <enumeration value = "CHANGE_OWNER_DISCONNECT" />
            <enumeration value = "NO_CHANGE_TARGET" />
            <enumeration value = "KICK" id = "0x80" />
            <enumeration value = "DISCONNECT" />
            <enumeration value = "REJOIN" />
        </restriction>
    </simpleType>
</packet>
